#Exercise 10.1
def add all(t):
  total = 0
  for x in t:
    total + = x
  return total

#Exercise 10.2
def cumsum(numbers):
  cum_list = []
  cum_sum = 0
  for num in numbers:
    cum_sum + = num
    cum_list.append(cum_sum)
  return cum_list
numbers = [1,2,3,4,5]
cumulative_sum = cumsum(numbers)
print(cumulative_sum)
